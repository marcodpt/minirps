{
  "port": 8080,
  "assets": "assets",
  "templates": "templates",
  "routes": [
    {
      "method": "GET",
      "path": "/",
      "template": "index.html"
    }, {
      "method": "GET",
      "path": "/form",
      "template": "form.html"
    }, {
      "method": "GET",
      "path": "/debug/:name",
      "template": "debug.html"
    }, {
      "method": "POST",
      "path": "/debug/:name",
      "template": "debug.html"
    }
  ]
}
